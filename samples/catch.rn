var three = func()
  panic("three!")

var two = func()
  var x = three?()
  print("two caught " $ tostr(x))

var one = func()
  catch x
    two()
  print("one caught " $ tostr(x))

var main = func()
  print("----")
  catch x
    print("before")
    one()
    print("after")
  print("main caught " $ tostr(x))

  print("----")
  print("before")
  x = one?()
  print("after")
  print("main caught " $ tostr(x))

  print("----")
  catch x
    print("before")
    panic("obvious")
    print("after")
  print("main caught " $ tostr(x))

  print("----")
  print("before")
  x = panic?("obvious")
  print("after")
  print("main caught " $ tostr(x))
